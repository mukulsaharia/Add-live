

<label class="heading_lable">Ads</label>

<table class="table table-hover" >
	<tr><td><b>Ads ID</b></td><td><b>Orgnization name</b></td><td><b>Category</b></td></tr>
	<% for i in @ads %>
	<tr><td><%= i.id %></td>
		<td><%= link_to i.orgname, :controller => :ads,:id => i.id %></td> 
		<td><%= i.category %></td></tr>
	<% end %>
</table>